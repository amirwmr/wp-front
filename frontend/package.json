		"@vite-pwa/sveltekit": "^1.1.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
