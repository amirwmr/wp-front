	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=24.0.0"
	},
	"overrides": {
		"cookie": "^0.7.2"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
